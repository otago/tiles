{
  "name": "tiles",
  "version": "1.0.0",
  "description": "SilverStripe tiles",
  "scripts": {
    "build": "yarn && NODE_ENV=production webpack -p --bail --progress",
    "build_windows": "yarn && set NODE_ENV=production && webpack  -p --bail --progress",
    "watch": "yarn && NODE_ENV=development webpack --watch --progress --debug",
    "watch_windows": "yarn && set NODE_ENV=development && webpack --watch --progress --debug",
    "css": "WEBPACK_CHILD=css npm run build",
    "lint": "eslint client/src; sass-lint -v",
    "test": "jest"
  },
  "keywords": [
    "silverstripe",
    "elemental",
    "content blocks"
  ],
  "author": "Otago Polytechnic",
  "license": "BSD-3-Clause",
  "dependencies": {
    "classnames": "^2.2.5",
    "jquery": "^3.5.0",
    "react": "16.7.0",
    "react-addons-test-utils": "15.6.2",
    "react-grid-layout": "^0.16.6",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@silverstripe/webpack-config": "^0.12.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.3.0",
    "jest-cli": "^19.0.2",
    "webpack": "^3.11.0",
    "webpack-cli": "^3.2.1"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "engines": {
    "node": "^6.x"
  },
  "externals": {
    "jquery": "jQuery"
  }
}
